<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>

<TabPane maxHeight="400" maxWidth="600" prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.leanapp.controller.MainController">
    <tabs>
        <Tab fx:id="profileTab" text="Profile">
            <content>
                <fx:include fx:id="profileTabPage" source="ProfileTabPage.fxml" />
            </content>
        </Tab>
        <Tab fx:id="weightLogTab" text="Weight Log">
            <content>
                <fx:include fx:id="weightLogTabPage" source="WeightTabPage.fxml" />
            </content>
        </Tab>
    </tabs>
</TabPane>
